<component
        name="ProjectRunConfigurationManager">
    <configuration
            default="false"
            name="UI + Server"
            type="com.intellij.execution.configurations.multilaunch"
            factoryName="MultiLaunchConfiguration">
        <rows>
            <ExecutableRowSnapshot>
                <option name="condition">
                    <ConditionSnapshot>
                        <option name="type"
                                value="immediately"/>
                    </ConditionSnapshot>
                </option>
                <option name="executable">
                    <ExecutableSnapshot>
                        <option name="id"
                                value="runConfig:.NET Launch Settings Profile.Clocktower.Server: http"/>
                    </ExecutableSnapshot>
                </option>
            </ExecutableRowSnapshot>
            <ExecutableRowSnapshot>
                <option name="condition">
                    <ConditionSnapshot>
                        <attributes>
                            <entry key="port"
                                   value="5120"/>
                        </attributes>
                        <option name="type"
                                value="waitPortOpened"/>
                    </ConditionSnapshot>
                </option>
                <option name="executable">
                    <ExecutableSnapshot>
                        <option name="id"
                                value="runConfig:npm.Launch UI"/>
                    </ExecutableSnapshot>
                </option>
            </ExecutableRowSnapshot>
        </rows>
        <method v="2"/>
    </configuration>
</component>